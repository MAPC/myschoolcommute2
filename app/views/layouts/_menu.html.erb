<div class="ui container">
  <div class="ui large secondary inverted menu">
    <%= link_to 'Home', root_path, class: 'item' %>
    <%= link_to 'Districts', districts_path, class: 'item' %>
    <div class="right menu">
      <% if current_user.nil? %>
        <%= link_to 'Sign In', new_user_session_path, class: 'item' %>
      <% else %>
        <%= link_to 'Profile', edit_user_registration_path, class: 'item' %>
        <%= link_to 'Log Out', destroy_user_session_path, method: 'delete', class: 'item' %>
        
      <% end %>
    </div>
  </div>
</div>